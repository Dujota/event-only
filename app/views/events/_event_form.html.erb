<%= form_for event do |f| %>
  <p>
    <%= f.label :name %> <br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :date %> <br>
    <%= f.date_field :date %>
  </p>

  <p>
    <%= f.label :capacity %><br>
    <%= f.number_field :capacity%>
  </p>

  <p>
    <%= f.label :city %><br>
    <%= f.text_field :city%>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>
